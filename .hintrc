{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off",
        "image-alt": "off"
      }
    ],
    "meta-viewport": "off"
  }
}